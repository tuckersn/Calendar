{
	"$schema": "https://turborepo.org/schema.json",
	"pipeline": {
		"build": {
			"cache": true,
			"dependsOn": ["^build"],
			"outputs": [
				"dist/**",
				"build/**"
			]
		},
		"test": {
			"cache": true
		},
		"format": {
			"cache": true,
			"inputs": [
				"src/**"
			]
		},
		"@internal/web#deploy": {
			"cache": true,
			"dependsOn": [
				"@internal/web#build",
				"@internal/server#build"
			]
		}
	}
}